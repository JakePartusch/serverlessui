  "dependencies": {
    "@serverlessui/domain-app": "^0.11.0",
    "@serverlessui/serverless-app": "^0.11.0",
    "aws-cdk": "1.115.0",
    "cosmiconfig": "^7.0.0",
    "esbuild": "^0.12.15",
    "gluegun": "^4.6.1",
    "tiny-glob": "^0.2.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.42",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
