{
  "name": "@jakepartusch/notlify-cli",
  "version": "0.0.9",
  "description": "cli CLI",
  "types": "dist/types/types.d.ts",
  "bin": {
    "notlify": "bin/cli"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,ts,tsx,json}",
    "lint": "tslint -p .",
    "compile": "tsc -p .",
    "build": "rm -rf ./dist && yarn compile",
    "prepublishOnly": "yarn compile",
    "test": "jest"
  },
  "files": [
    "tsconfig.json",
    "tslint.json",
    "dist",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@jakepartusch/notlify-application": "0.0.5",
    "glob": "^7.1.6",
    "gluegun": "latest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "jest": "^24.1.0",
    "prettier": "^1.12.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
